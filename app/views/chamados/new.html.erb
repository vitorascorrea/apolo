<%= form_for(@chamado, html: {multipart: true}) do |f| %>

  Nota <br>
  <%= f.number_field :nota %>
  <%= @chamado.errors.full_messages_for(:nota).to_sentence %><br><br>

  PDF da NCF <br>
  <%= f.file_field :ncf, accept: 'application/pdf' %>
  <%= @chamado.errors.full_messages_for(:ncf).to_sentence %><br><br>

  <%= f.submit "Enviar" %>

<% end %>
